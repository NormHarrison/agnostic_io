project Ada_Agnostic_IO is

   for Source_Dirs            use ("src");
   for Ignore_Source_Sub_Dirs use ("wip");
   for Object_Dir             use "obj";

   package Compiler is
      for Switches ("Ada") use
        ("-gnatwae",
         "-g");
   end Compiler;

   package Binder is
      for Switches ("Ada") use
        ("-E");
   end Binder;
   
end Ada_Agnostic_IO;
